{
  "name": "nft-content-authentication-platform",
  "version": "1.0.0",
  "description": "A decentralized platform for authenticating and managing digital content using blockchain technology, IPFS storage, and NFT minting capabilities.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd frontend && npm run build",
    "install-frontend": "cd frontend && npm install",
    "start-frontend": "cd frontend && npm start",
    "setup": "npm install && npm run install-frontend",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js --network sepolia"
  },
  "keywords": [
    "nft",
    "blockchain",
    "ipfs", 
    "content-authentication",
    "ethereum",
    "web3",
    "decentralized",
    "hardhat"
  ],
  "author": "Taha Sohail",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tahasohail12/contracts.git"
  },
  "bugs": {
    "url": "https://github.com/tahasohail12/contracts/issues"
  },
  "homepage": "https://github.com/tahasohail12/contracts#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "chai": "^4.5.0",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.1",
    "hardhat-gas-reporter": "^2.3.0",
    "nodemon": "^3.1.7"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "form-data": "^4.0.4",
    "express": "^4.21.0",
    "mongoose": "^8.7.3",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "@helia/unixfs": "^4.0.1",
    "helia": "^5.1.0"
  }
}
